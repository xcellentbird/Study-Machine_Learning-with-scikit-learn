   "outputs": [],
   "source": [
    "A = np.array([[-3, 1], [-1, 1], [1, 1], [3, 1]])\n",
    "b = np.array([[-1],[-1],[3],[3]])\n",
    "\n",
    "At = A.transpose()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "a96396d6",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([[0.8],\n",
       "       [1. ]])"
      ]
     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "x = np.linalg.inv(At @ A) @ At` @ b\n",
    "x"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "0906f696",
   "metadata": {},
   "outputs": [],
   "source": [
